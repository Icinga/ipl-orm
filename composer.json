{
  "name": "ipl/orm",
  "type": "library",
  "description": "Icinga PHP Library - ORM",
  "license": "MIT",
  "keywords": [
    "sql",
    "database",
    "orm"
  ],
  "homepage": "https://github.com/Icinga/ipl-orm",
  "require": {
    "php": ">=5.6.0",
    "ipl/sql": "dev-master",
    "ipl/stdlib": "dev-master",
    "ext-pdo": "*",
    "cweagans/composer-patches": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "ipl\\Orm\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ipl\\Tests\\Orm\\": "tests"
    }
  },
  "extra": {
    "patches": {
      "ipl/stdlib": {
        "Add Str::camel()": "patches/ipl/stdlib/str_camel.patch"
      }
    }
  }
}
